name: "multi_task"
platform: "tensorrt_plan"
max_batch_size: 4
default_model_filename: "multi_task.onnx_b4_gpu0_fp16.engine"
input [
  {
    name: "input_1"
    data_type: TYPE_FP32
    dims: [ 3, 80, 60 ]
  }
]
output [
  {
    name: "season/Softmax"
    data_type: TYPE_FP32
    dims: [ 4, 1, 1 ]
  },
  {
    name: "category/Softmax"
    data_type: TYPE_FP32
    dims: [ 10, 1, 1 ]
  },
  {
    name: "base_color/Softmax"
    data_type: TYPE_FP32
    dims: [ 11, 1, 1 ]
  }
]

instance_group [
  {
    kind: KIND_GPU
    count: 1
    gpus: 0
  }
]
